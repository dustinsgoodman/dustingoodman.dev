{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/components/*": ["src/components/*"],
			"@/images/*": ["src/images/*"],
			"@/layouts/*": ["src/layouts/*"]
		},
		"allowJs": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"strictNullChecks": true
	}
}
